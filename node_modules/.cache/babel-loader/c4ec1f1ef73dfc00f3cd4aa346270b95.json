{"ast":null,"code":"import MainContentMenus from './MainContentMenu.vue';\nimport ProductArea from '../product/ProductAre.vue';\nimport CategoriesArea from '../category/CategoriesArea.vue';\nimport { eventBus } from '../../main';\nexport default {\n  data: function () {\n    return {\n      activeMenu: {}\n    };\n  },\n  created() {\n    eventBus.$on(\"pageData\", name => {\n      this.activeMenu = name;\n    });\n  },\n  components: {\n    MainContentMenus,\n    ProductArea,\n    CategoriesArea\n  }\n};","map":{"version":3,"mappings":"AASA;AACA;AACA;AACA;AAEA;EACAA;IACA;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACAC;IACAC;IACAC;EACA;AACA","names":["data","activeMenu","created","eventBus","components","MainContentMenus","ProductArea","CategoriesArea"],"sourceRoot":"src/components/ContentArea","sources":["MainContent.vue"],"sourcesContent":["<template>\n    <div id=\"content\">\n        <MainContentMenus></MainContentMenus>   \n        <ProductArea v-if=\"activeMenu.title === 'Ürünler'\"></ProductArea>\n        <CategoriesArea v-else :tree-data=\"this.tree\"></CategoriesArea>\n    </div>\n</template>\n\n<script>\nimport MainContentMenus from './MainContentMenu.vue'\nimport ProductArea from '../product/ProductAre.vue'\nimport CategoriesArea from '../category/CategoriesArea.vue'\nimport {eventBus} from '../../main'\n\nexport default {\n    data : function(){\n        return{\n            activeMenu : {},\n        }\n    },\n    created(){\n        eventBus.$on(\"pageData\", (name) =>{\n            this.activeMenu = name;\n        })\n    },\n    components : {\n        MainContentMenus,\n        ProductArea,\n        CategoriesArea\n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}