r-2\"\n        @click=\"editItem(item)\"\n      >\n        mdi-pencil\n      </v-icon>\n      <v-icon\n        small\n        @click=\"deleteItem(item)\"\n      >\n        mdi-delete\n      </v-icon>\n    </template>\n        </v-data-table>\n        <v-dialog\n        v-model=\"dialog\"\n        persistent\n        max-width=\"600px\"\n      >\n        <v-card>\n          <v-card-title>\n            <span class=\"text-h5\">{{selected.name}}</span>\n          </v-card-title>\n          <v-card-text>\n            <v-container>\n              <v-row>\n                <v-col cols=\"12\" sm=\"6\" md=\"6\">\n                  <v-text-field\n                    label=\"Ürün Adı\"\n                    required\n                    v-model=\"editedItem.name\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"6\" md=\"6\">\n                  <v-text-field\n                    label=\"Barkod\"\n                    v-model=\"editedItem.barcode\"\n                  ></v-text-field>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col cols=\"12\" sm=\"6\" md=\"6\">\n                  <v-text-field\n                    label=\"Ürün Adedi\"\n                    required\n                    v-model=\"editedItem.quantity\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"6\" md=\"6\">\n                  <v-text-field\n                    label=\"Stok\"\n                    v-model=\"editedItem.stok\"\n                  ></v-text-field>\n                </v-col>\n              </v-row>\n              <v-row>\n                <v-col cols=\"12\" sm=\"6\" md=\"6\">\n                  <v-text-field\n                    label=\"Ürün Fiyatı\"\n                    required\n                    v-model=\"editedItem.price\"\n                  ></v-text-field>\n                </v-col>\n                <v-col cols=\"12\" sm=\"6\" md=\"6\">\n   